@prefix doap:  <http://usefulinc.com/ns/doap#>.
@prefix foaf:  <http://xmlns.com/foaf/0.1/>.
@prefix lv2:   <http://lv2plug.in/ns/lv2core#>.
@prefix mod:   <http://moddevices.com/ns/mod#>.
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://aidadsp.cc/plugins/aidadsp-bundle/rt-neural-generic>
a lv2:Plugin, lv2:UtilityPlugin;

doap:name "rt-neural-generic";

doap:developer [
    foaf:name "Aida DSP";
    foaf:homepage <http://aidadsp.cc>;
];

doap:maintainer [
    foaf:name "Aida DSP";
    foaf:homepage <http://aidadsp.cc>;
];

mod:brand "Aida DSP";
mod:label "rt-neural-generic";

doap:license "GPL";

rdfs:comment """
Simple player for lstm models using RTNeural inference engine
""";

lv2:port
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "In";
    lv2:name "In";
    lv2:shortname "In";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "Out";
    lv2:name "Out";
    lv2:shortname "Out";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "Param1";
    lv2:name "Param1";
    lv2:shortname "P1";
    lv2:default 0.5;
    lv2:minimum 0;
    lv2:maximum 1.0;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "Param2";
    lv2:name "Param2";
    lv2:shortname "P2";
    lv2:default 0.5;
    lv2:minimum 0;
    lv2:maximum 1.0;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "Master";
    lv2:name "Master";
    lv2:shortname "Master";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1.0;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 5;
    lv2:symbol "Bypass";
    lv2:name "Bypass";
    lv2:shortname "Bypass";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:portProperty lv2:integer;
    lv2:portProperty lv2:toggled;
].
